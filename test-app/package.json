{
  "name": "test-app",
  "version": "4.4.0",
  "description": "Ember addon for Litepicker date range picker library.",
  "keywords": [
    "litepicker",
    "ember-litepicker",
    "datepicker",
    "datetime picker",
    "daterange picker"
  ],
  "repository": "https://github.com/sinankeskin/ember-litepicker",
  "license": "MIT",
  "author": "Sinan Keskin <sinan@kesk.in> (https://kesk.in)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "deploy": "npm version patch && git push && git push origin --tags && npm publish",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "update": "ecu --to latest && ncu -u && npm install && npm run lint:fix && npm run lint && git commit -am \"Update some packages\" && git push",
    "update:all": "run-s update deploy"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.47.2",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.2.4",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^6.0.0",
    "ember-cli": "~3.28.4",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-page-object": "^1.17.9",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-litepicker": "4.4.0",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "^4.0.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.14.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.64.4"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  }
}
